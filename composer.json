{
    "name": "somenergia/plataforma",
    "version": "1.10.6",
    "description": "Plataforma Som Energia",
    "license": "GPL-2.0",
    "type": "project",
    "require": {
        "php": ">=5.6",
	"ext-mysql": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-xml": "*",
        "mrclay/minify": "~2.0",
        "knplabs/gaufrette": "~0.1.0",
        "tedivm/stash": "~0.12",
        "ircmaxell/password-compat": "~1.0",
        "roave/security-advisories": "dev-master",
	"beck24/elgg_recaptcha": "1.1.1",
	"au/au_subgroups": "2.0.0",
	"coldtrick/file_tools": "v2.0",
	"beck24/spam_login_filter": "2.1.1",
	"blackleg/threads": "1.8.3",
        "elgg/mentions": "dev-master",
        "coldtrick/group_tools": "v3.2"
    },
    "scripts": {
        "test": "phpunit",
        "lint": [
            "phpcs --standard=vendor/elgg/sniffs/elgg.xml --warning-severity=0 --ignore=*/tests/*,*/upgrades/*,*/deprecated* engine/classes engine/lib",
            "composer validate"
        ]
    },
    "suggest": {
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-0": {
            "": "engine/classes/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "engine/tests/phpunit/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "elgg/sniffs": "dev-master",
        "squizlabs/php_codesniffer": "~1.5",
        "simpletest/simpletest": "~1.1"
    }
}
