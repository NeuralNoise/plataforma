{
    "name": "somenergia/plataforma",
    "version": "1.12.15",
    "description": "Plataforma Som Energia",
    "license": "GPL-2.0",
    "type": "project",
    "require": {
	"symfony/http-foundation": "~2.6",
        "mrclay/minify": "~2.0",
        "knplabs/gaufrette": "~0.1.0",
        "tedivm/stash": "0.14.*",
        "ircmaxell/password-compat": "~1.0",
        "roave/security-advisories": "dev-master",
        "elgg/login_as": "~1.9",  
	"beck24/elgg_recaptcha": "1.1.1",
	"au/au_subgroups": "2.0.0",
	"coldtrick/file_tools": "v2.0",
	"beck24/spam_login_filter": "2.1.1",
	"blackleg/threads": "1.9.2",
        "elgg/mentions": "dev-master",
        "coldtrick/group_tools": "v6.0.3",
        "blackleg/rm_group_reports": "1.9.0",
        "blackleg/rm_group_default_fields": "1.1.0",
        "blackleg/rm_group_autosubscription": "1.9",
        "iionly/tidypics": "1.10.11"
    },
    "scripts": {
        "test": "phpunit",
        "lint": [
            "phpcs --standard=vendor/elgg/sniffs/elgg.xml --warning-severity=0 --ignore=*/tests/*,*/upgrades/*,*/deprecated* engine/classes engine/lib",
            "composer validate"
        ]
    },
    "suggest": {
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-0": {
            "": "engine/classes/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "engine/tests/phpunit/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "elgg/sniffs": "dev-master",
        "squizlabs/php_codesniffer": "*",
        "simpletest/simpletest": "*"
    }
}
